{
  "cli": {
    "version": ">= 14.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "GOOGLE_SERVICES_JSON": "@env:GOOGLE_SERVICES_JSON",
        "GOOGLE_SERVICES_PLIST": "@env:GOOGLE_SERVICES_PLIST"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
